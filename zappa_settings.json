{
    "prod": {
        "app_function": "application.app",
        "aws_region": "ap-northeast-1",
        "profile_name": "default",
        "project_name": "japanese-study",
        "runtime": "python3.9",
        "s3_bucket": "japanese-study-lambda-deployments",
        "vpc_config": {
            "SubnetIds": [
                "${private_subnet_1}",
                "${private_subnet_2}"
            ],
            "SecurityGroupIds": [
                "${lambda_security_group}"
            ]
        },
        "environment_variables": {
            "FLASK_ENV": "production",
            "DB_HOST": "${aurora_endpoint}",
            "DB_NAME": "japanese_study",
            "DB_USER": "admin"
        },
        "manage_roles": false,
        "role_name": "japanese-study-prod-lambda-role",
        "apigateway_enabled": false,
        "keep_warm": false,
        "lambda_description": "Japanese Study Application API"
    }
}